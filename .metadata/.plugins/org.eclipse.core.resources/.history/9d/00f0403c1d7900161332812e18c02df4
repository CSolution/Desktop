package br.com.onpecas.model;

import java.sql.Connection;
import java.sql.ResultSet;
import java.sql.SQLException;
import java.util.ArrayList;
import java.util.List;

import br.com.onpecas.helper.MySqlConnect;

public class Permissao {
	
	private int Oid_permissao;
	private int acessar;
	private int inserir;
	private int editar;
	private int excluir;
	private String tela;

	public int getOid_permissao() {
		return Oid_permissao;
	}
	public void setOid_permissao(int oid_permissao) {
		Oid_permissao = oid_permissao;
	}
	public String getTela() {
		return tela;
	}
	public void setTela(String tela) {
		this.tela = tela;
	}
	public int getAcessar() {
		return acessar;
	}
	public void setAcessar(int acessar) {
		this.acessar = acessar;
	}
	public int getInserir() {
		return inserir;
	}
	public void setInserir(int inserir) {
		this.inserir = inserir;
	}
	public int getEditar() {
		return editar;
	}
	public void setEditar(int editar) {
		this.editar = editar;
	}
	public int getExcluir() {
		return excluir;
	}
	public void setExcluir(int excluir) {
		this.excluir = excluir;
	}
	
	
}
